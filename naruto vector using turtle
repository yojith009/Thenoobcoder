def curve1(a,d):
    for i in range(d):
        Naruto.right(a)
        Naruto.forward(1)

def curve2(a,d):
    for i in range(d):
        Naruto.left(a)
        Naruto.forward(1)

curve1(0.1,260)
curve1(0.2,80)
